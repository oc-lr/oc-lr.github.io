{
  "name": "oclr",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "npm": "^6.1.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.3.3",
    "tachyons": "^4.10.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "compile_tailwind": "tailwind build ./src/styles/tailwind-config.css -c ./tailwind.js -o ./src/styles/tailwind.css",
    "build": "npm run compile_tailwind && gatsby build",
    "deploy": "npm run compile_tailwind && gatsby build --prefix-paths && gh-pages -d public -b master",
    "develop": "npm run compile_tailwind && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "prettier": "^1.12.0",
    "tailwindcss": "^0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
